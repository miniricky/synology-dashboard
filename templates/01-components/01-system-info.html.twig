<div class="section-row" id="system-info">
  <div class="container-fluid">
    <div class="info-wrapper">
      <h2 class="h6">System Information</h2>

      <div class="info model">
        <span>Model:</span>
        <span class="model-info"></span>
      </div>

      <div class="info processor">
        <span>Processor:</span>
        <span class="processor-info"></span>
      </div>

      <div class="info cores">
        <span>Cores:</span>
        <span class="cores-info"></span>
      </div>

      <div class="info firmware">
        <span>Firmware:</span>
        <span class="firmware-info"></span>
      </div>

      <div class="info ram">
        <div class="ram-used">
          <div class="progress" role="progressbar" aria-label="Example with label" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
            <div class="progress-bar"></div>
          </div>
        </div>

        <div class="ram-total">
          <span>RAM</span>
          <span class="ram-info"></span>
        </div>
      </div>

      <div class="info hdd">
        <div class="hdd-used">
          <div class="progress" role="progressbar" aria-label="Example with label" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
            <div class="progress-bar"></div>
          </div>
        </div>

        <div class="hdd-total">
          <span>HDD</span>
          <span class="hdd-info"></span>
        </div>
      </div>
    </div>
  </div>
</div>